RewriteEngine On
# CSS
RewriteRule ^css/(.*\.css)$ assets/css/$1 [L,NC]
# JS
RewriteRule ^js/(.*\.js)$ assets/js/$1 [L,NC]
# IMAGES
RewriteRule ^images/(.*\.png)$ assets/images/$1 [L,NC]
# INDEX
RewriteRule ^login$ index.php [L,NC]
# RECOVERY
RewriteRule ^recovery=(.*)$ index.php?id-recovery=$1 [L,NC]
# REGISTER
RewriteRule ^sign-up$ register.php [L,NC]
# HOME
RewriteRule ^home$ home.php [L,NC]
# GAME